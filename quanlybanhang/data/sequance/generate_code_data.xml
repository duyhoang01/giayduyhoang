<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="sale_order_sequence">
            <field name="name">Sequence For Sale order</field>
            <field name="code">sale.order.sequence</field>
            <field name="active">TRUE</field>
            <field name="padding">3</field>
            <field name="prefix">SO</field>
            <field name="number_increment">1</field>
            <field name="number_next">6</field>
            <field name="implementation">standard</field>
        </record>
    </data>

    <data noupdate="1">
        <record model="ir.sequence" id="group_product_sequence">
            <field name="name">Sequence For Group Product</field>
            <field name="code">group.product.sequence</field>
            <field name="active">TRUE</field>
            <field name="padding">2</field>
            <field name="prefix">G</field>
            <field name="number_increment">1</field>
            <field name="number_next">12</field>
            <field name="implementation">standard</field>
        </record>
    </data>
</odoo>